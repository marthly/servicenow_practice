        <condition/>
        <description>(credit application) &lt;City&gt; - Displays the city that corresponds to the selected State.</description>
        <field>u_state</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>State-City Match</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }
	
	var city = g_form.getValue('u_city');
	
	g_form.clearOptions('u_city');

   //Type appropriate comment here, and begin script below
	if(newValue === 'alabama' || oldValue === 'alabama'){
		
		g_form.addOption('u_city', 'auburn' , 'Auburn');
		g_form.addOption('u_city', 'birmingham', 'Birmingham');
		g_form.addOption('u_city', 'montgomery', 'Montgomery');
		g_form.addOption('u_city', 'huntsville', 'Huntsville');
		g_form.addOption('u_city', 'mobile', 'Mobile');
		g_form.addOption('u_city', 'tuscaloosa', 'Tuscaloosa');
		g_form.addOption('u_city', 'hoover', 'Hoover');
		g_form.addOption('u_city', 'dothan', 'Dothan');
		g_form.addOption('u_city', 'decatur', 'Decatur');
		g_form.addOption('u_city', 'madison', 'Madison');
		g_form.addOption('u_city', 'florence', 'Florence');
		g_form.addOption('u_city', 'phenix_city', 'Phenix City');
		g_form.addOption('u_city', 'prattville', 'Prattville');
		g_form.addOption('u_city', 'gadsden', 'Gadsden');
		g_form.addOption('u_city', 'vestavia_hills', 'Vestavia Hills');
		g_form.addOption('u_city', 'other_city', 'Other City');
	}
   
}]]></script>
